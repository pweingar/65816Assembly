=$b00000				SP0_VRAM = $B00000                      ; This is where we will save the sprite bitmap... beginning of VRAM
.010000	e2 20		sep #$20	START           SEP #$20                ; Set the accumulator to 8-bit wide
.010002	c2 10		rep #$10	                REP #$10                ; Set the index registers to 16-bits wide
.010004					INITLUT
.010004	a2 00 00	ldx #$0000	                LDX #0                  ; Set X to point to the first byte
.010007	bf 4f 04 01	lda $01044f,x	LUTLOOP         LDA LUT_START,X         ; Get a byte of the color table
.01000b	9f 00 20 af	sta $af2000,x	                STA GRPH_LUT0_PTR,X     ; Save it to Vicky's Graphics LUT#0
.01000f	e8		inx		                INX                     ; Move to the next byte
.010010	e0 00 04	cpx #$0400	                CPX #256*4              ; Check to see if we've reached the limit
.010013	d0 f2		bne $010007	                BNE LUTLOOP             ; No: copy this next byte too
.010015					INITBITMAP
.010015	a2 00 00	ldx #$0000	                LDX #0                  ; Set X to point to the first byte
.010018	bf 4f 00 01	lda $01004f,x	BITMAPLOOP      LDA IMG_START,X         ; Get a byte of bitmap data
.01001c	9f 00 00 b0	sta $b00000,x	                STA SP0_VRAM,X          ; Save it to Video RAM
.010020	e8		inx		                INX                     ; Move to the next byte
.010021	e0 00 04	cpx #$0400	                CPX #32*32              ; Check to see if we've reached the limit
.010024	d0 f2		bne $010018	                BNE BITMAPLOOP          ; No: copy this next byte too
.010026					SHOWSPRITE
.010026	a9 27		lda #$27	                LDA #Mstr_Ctrl_Text_Mode_En | Mstr_Ctrl_Text_Overlay | Mstr_Ctrl_Graph_Mode_En | Mstr_Ctrl_Sprite_En
.010028	8f 00 00 af	sta $af0000	                STA MASTER_CTRL_REG_L
.01002c	a9 01		lda #$01	                LDA #$01                ; Enable Sprite #0
.01002e	8f 00 0c af	sta $af0c00	                STA SP00_CONTROL_REG
.010032	a9 00		lda #$00	                LDA #0                  ; Tell Vicky that the sprite is at the start of VRAM
.010034	8f 01 0c af	sta $af0c01	                STA SP00_ADDY_PTR_L
.010038	8f 02 0c af	sta $af0c02	                STA SP00_ADDY_PTR_M
.01003c	8f 03 0c af	sta $af0c03	                STA SP00_ADDY_PTR_H
